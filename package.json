{
  "name": "vscode-user-status",
  "author": "timmyCoder",
  "repository": {
    "type": "git",
    "url": "https://github.com/timmyCoder/vscode-user-status"
  },
  "bugs": {
    "url": "https://github.com/timmyCoder/vscode-user-status/issues"
  },
  "version": "0.1.3",
  "main": "./out/extension.js",
  "scripts": {
    "compile": "tsc -p ./",
    "generate": "graphql-codegen --config codegen.yml",
    "test": "jest",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.4",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-context": "^1.0.19",
    "apollo-link-http": "^1.5.16",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "node-fetch": "^2.6.0",
    "type-fest": "^0.11.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.12.2",
    "@graphql-codegen/introspection": "1.12.2",
    "@graphql-codegen/typescript": "^1.12.2",
    "@graphql-codegen/typescript-operations": "^1.12.2",
    "@types/jest": "^25.1.3",
    "@types/node": "^10.14.17",
    "@types/vscode": "^1.34.0",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "all-contributors-cli": "^6.14.0",
    "codecov": "^3.6.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-xo-space": "^0.24.0",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.1",
    "eslint-plugin-tsdoc": "^0.2.3",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "prettier-package-json": "^2.1.3",
    "ts-jest": "^25.2.1",
    "typescript": "^3.5.1",
    "vsce": "^1.73.0"
  },
  "keywords": [
    "github",
    "slack",
    "user-status"
  ],
  "engines": {
    "vscode": "^1.34.0"
  },
  "activationEvents": [
    "*"
  ],
  "categories": [
    "Other"
  ],
  "contributes": {
    "configuration": {
      "title": "User Status",
      "properties": {
        "userStatus.language": {
          "scope": "window",
          "description": "Language used",
          "enum": [
            "en",
            "ja"
          ],
          "default": "en"
        },
        "userStatus.priority": {
          "scope": "window",
          "description": "Which status should be given priority at startup",
          "enum": [
            "github",
            "slack"
          ],
          "default": "github"
        },
        "userStatus.slack": {
          "scope": "window",
          "description": "Settings that about Slack",
          "oneOf": [
            {
              "type": "object",
              "properties": {
                "enable": {
                  "description": "Enable Slack feature",
                  "type": "boolean",
                  "default": false
                },
                "user": {
                  "description": "Your member id in a workspace",
                  "type": "string",
                  "examples": [
                    "U..."
                  ]
                },
                "accessToken": {
                  "description": "Issued Slack's oauth-access-token",
                  "type": "string",
                  "examples": [
                    "xoxp-...-...-......"
                  ]
                }
              },
              "required": [
                "enable"
              ]
            },
            {
              "type": "object",
              "properties": {
                "enable": {
                  "description": "Enable Slack feature",
                  "type": "boolean",
                  "default": false
                },
                "workspaces": {
                  "description": "The settings about multiple workspaces",
                  "type": "array",
                  "minItems": 1,
                  "items": {
                    "type": "object",
                    "properties": {
                      "user": {
                        "description": "Your member id in a workspace",
                        "type": "string",
                        "examples": [
                          "U..."
                        ]
                      },
                      "accessToken": {
                        "description": "Issued Slack's oauth-access-token",
                        "type": "string",
                        "examples": [
                          "xoxp-...-...-......"
                        ]
                      }
                    }
                  }
                }
              },
              "required": [
                "enable"
              ]
            }
          ]
        },
        "userStatus.github": {
          "scope": "window",
          "description": "Settings that about GitHub",
          "type": "object",
          "properties": {
            "enable": {
              "description": "Enable GitHub feature",
              "type": "boolean",
              "default": false
            },
            "username": {
              "description": "Your GitHub user name",
              "type": "string"
            },
            "accessToken": {
              "description": "Issued GitHub's personal-access-token",
              "type": "string"
            }
          },
          "required": [
            "enable"
          ]
        },
        "userStatus.status": {
          "scope": "window",
          "description": "Additional status list",
          "type": "object",
          "additionalProperties": {
            "type": "object",
            "properties": {
              "slack": {
                "type": "object",
                "properties": {
                  "emojiName": {
                    "type": "string"
                  }
                },
                "required": [
                  "emojiName"
                ]
              },
              "github": {
                "type": "object",
                "properties": {
                  "emojiName": {
                    "type": "string"
                  }
                },
                "required": [
                  "emojiName"
                ]
              },
              "text": {
                "type": "string"
              },
              "priority": {
                "type": "number",
                "minimum": 0,
                "default": 100
              }
            },
            "required": [
              "slack",
              "github",
              "text",
              "priority"
            ]
          }
        }
      }
    },
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "displayName": "User Status",
  "icon": "icon.png"
}
